{
    "compilerOptions": {
      "target": "ES6",
      "module": "commonjs",
      "strict": false,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "outDir": "./dist"
    },
    "include": ["src/**/*.ts", "server.ts"],
    "exclude": ["node_modules"],
    "typeRoots": [
      "./node_module/@types",
      "./src/types"
    ],
  }
  